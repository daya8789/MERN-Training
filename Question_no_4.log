function rotate(){
    var input = "brainmentors";
    var array = input.split("");
    var step = array.length;
    var arr = array.splice(-1);
    var arr1 = array.splice(0, step-1);
    var array = arr.concat(arr1);
    var output = array.join("");
    console.log(output);
}
