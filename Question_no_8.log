// 8. Write a JS Function to validate Phone Number 
//    Only number and must be 10 digit long

    //   Solution

function mobile(){
    var s = prompt("");
    var num = parseInt(s);
    console.log(num);
    var c=0;
    while(num!=0){
        num%10;
        var num = Math.floor(num/10);
        c++;
    }
    if(c==10){
        
        console.log("Number is Valid");
        
    }
    else{
        
        console.log("Number is Not Valid");
    }
    
}

